<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCar"
        tools:targetApi="31">
        <activity
            android:name=".activities.ServiceActivity"
            android:exported="false" />
        <activity
            android:name=".activities.RefuelingActivity"
            android:exported="false" />
        <activity
            android:name=".activities.CarDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AdditionalCarActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MileageActivity"
            android:exported="false" />
        <activity
            android:name=".activities.ModelCarActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MarkCarActivity"
            android:exported="false" />
        <activity
            android:name=".activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".activities.SignInActivity"
            android:exported="false" />
        <activity
            android:name=".activities.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".activities.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>